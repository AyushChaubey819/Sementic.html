/* ============================
   Social Media Icons in a Row
   ============================ */

/* Common styles for all icons */
.widget a i,
.widget a svg {
    width: 50px;
    height: 50px;
    font-size: 22px;
    padding: 10px;
    border-radius: 50%;
    display: inline-flex;
    align-items: center;
    justify-content: center;
    color: #fff !important;
    fill: #fff !important;
    transition: all 0.3s ease;
}

/* Background colors for each platform */
.widget a[href*="instagram.com"] i,
.widget a[href*="instagram.com"] svg {
    background-color: #E1306C !important; /* Instagram */
	
}

.widget a[href*="linkedin.com"] i,
.widget a[href*="linkedin.com"] svg {
    background-color: #0A66C2 !important; /* LinkedIn */
	
}

.widget a[href*="t.me"] i,
.widget a[href*="t.me"] svg {
    background-color: #2AABEE !important; /* Telegram */
	
}

.widget a[href*="youtube.com"] i,
.widget a[href*="youtube.com"] svg {
    background-color: #FF0000 !important; /* YouTube */

}

/* Hover effect */
/* .widget a:hover i,
.widget a:hover svg {
    transform: scale(1.1);
    opacity: 0.9;
} */

/* Align icons in a horizontal row */
.widget {
    display: flex !important;
    flex-direction: row !important;
/*     justify-content: center !important; */
    align-items: center !important;
    flex-wrap: nowrap !important;
    gap: 20px !important; /* space between icons */
