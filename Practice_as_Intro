<body>

<h1>HTML Program Set</h1>

<!-- 1 Heading -->
<h2>1. Heading</h2>
<h1>Hello Ayush</h1>

<!-- 2 Paragraph -->
<h2>2. Paragraph</h2>
<p>This is a paragraph.</p>

<!-- 3 Bold -->
<h2>3. Bold</h2>
<b>Bold Text</b>

<!-- 4 Italic -->
<h2>4. Italic</h2>
<i>Italic Text</i>

<!-- 5 Underline -->
<h2>5. Underline</h2>
<u>Underlined Text</u>

<!-- 6 Image -->
<h2>6. Image</h2>
<img src="a.jpg" width="120">

<!-- 7 Link -->
<h2>7. Hyperlink</h2>
<a href="#">Click Here</a>

<!-- 8 Ordered List -->
<h2>8. Ordered List</h2>
<ol><li>Item 1</li><li>Item 2</li></ol>

<!-- 9 Unordered List -->
<h2>9. Unordered List</h2>
<ul><li>A</li><li>B</li></ul>

<!-- 10 Table -->
<h2>10. Table</h2>
<table border="1"><tr><td>A</td><td>B</td></tr></table>

<!-- 11 Text Input -->
<h2>11. Text Input</h2>
<input type="text" placeholder="Type here">

<!-- 12 Password Input -->
<h2>12. Password</h2>
<input type="password">

<!-- 13 Radio -->
<h2>13. Radio Button</h2>
<input type="radio" name="a"> Yes

<!-- 14 Checkbox -->
<h2>14. Checkbox</h2>
<input type="checkbox"> Accept

<!-- 15 Select Box -->
<h2>15. Dropdown</h2>
<select><option>Option 1</option></select>

<!-- 16 Textarea -->
<h2>16. Textarea</h2>
<textarea rows="2" cols="30"></textarea>

<!-- 17 Button -->
<h2>17. Button</h2>
<button>Click</button>

<!-- 18 Inline Styling -->
<h2>18. Inline CSS</h2>
<p style="color:blue;">Blue Text</p>

<!-- 19 Div Box -->
<h2>19. Div</h2>
<div style="border:1px solid; padding:5px;">Box</div>

<!-- 20 Span -->
<h2>20. Span</h2>
<p>Hello <span style="color:red;">Ayush</span></p>

<!-- 21 Audio -->
<h2>21. Audio</h2>
<audio controls></audio>

<!-- 22 Video -->
<h2>22. Video</h2>
<video controls width="200"></video>

<!-- 23 Marquee -->
<h2>23. Marquee</h2>
<marquee>Scrolling Text</marquee>

<!-- 24 Comment -->
<h2>24. Comment</h2>
<!-- This is a comment -->

<!-- 25 Background Color -->
<h2>25. Background Color</h2>
<div style="background:yellow;">Yellow BG</div>

<!-- 26 Center Text -->
<h2>26. Center Text</h2>
<h3 style="text-align:center;">Centered</h3>

<!-- 27 Line Break -->
<h2>27. Line Break</h2>
Line1<br>Line2

<!-- 28 Horizontal Line -->
<h2>28. Horizontal Rule</h2>
<hr>

<!-- 29 Mark Tag -->
<h2>29. Mark Tag</h2>
<p><mark>Highlighted</mark></p>

<!-- 30 Small Tag -->
<h2>30. Small Tag</h2>
<p><small>Small Text</small></p>

</body>
